.homelist{
  width:100%;
  white-space: nowrap;
  font-size:15rpx;
  position: fixed;
  top: 0;
  z-index:1;
  background-color: white;
}

.cellDetail{
  border-bottom: 1rpx solid #ccc;
}
.cellTopContent{
  display: flex;
  /* justify-content: center; */
  flex-direction: row;
  width:100%;
  height: 70rpx;
  position:relative;
  align-items: center;  
}

.cellTopLeft{
  text-align: left;
  width: 80%;
  font-family: "黑体";
  color: #000;
  font-size: 30rpx;
  margin-left: 20rpx; 
}
.cellTopRight{
  font-family:"楷体";
  color:#fff;
  background-color:#00a0ac;
  font-size:25rpx;
  padding-left:10rpx;
  padding-right:10rpx;
}
.cellBottomContent{
  height: 50rpx;
  background-color: #f6f6f6;
  margin: 0rpx 20rpx 10rpx 20rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  color:gray;
}
.cellBottomLeft{
  font-size: 25rpx;
  text-align: left;
  margin-left: 20rpx;
}
.cellBottomRight{
  font-size: 25rpx;
  text-align: right;
  width: 50%;
  margin-right: 20rpx;
}
.bottomView {
  width: 90%;
  font-size: 35rpx;
  background-color: #00a0ac;
  color: #fff;
  position: fixed;
  bottom: 15rpx;
  height: 2.0;
  line-height: 2.0;
  margin: 0 30rpx 0 30rpx;
  align-items: center;
  text-align: center;
}